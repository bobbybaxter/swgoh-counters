{
  "name": "swgoh-counters",
  "homepage": "https://swgohcounters.com",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "proxy": "http://127.0.0.1:5000",
  "scarfSettings.enabled": false,
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "firebase": "^7.15.2",
    "history": "^4.10.1",
    "jquery": "^3.5.1",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^2.7.0",
    "react-meta-tags": "^0.7.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "react-table": "^7.2.1",
    "reactstrap": "^8.4.1"
  },
  "scripts": {
    "start": "react-scripts start --ignore server",
    "build": "react-scripts build",
    "postbuild": "react-snap --disable-web-security",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "npm run build && gh-pages -d build",
    "server": "LOG_LEVEL=debug nodemon server/index.js | pino-pretty",
    "dev": "concurrently \"npm run server\" \"npm start\"",
    "client:deploy": "npm run build && aws s3 sync build/ s3://swgohcounters.com"
  },
  "skipThirdPartyRequests": true,
  "reactSnap": {
    "puppeteerArgs": [
      "--disable-web-security"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "pino-pretty": "^4.0.0",
    "react-snap": "^1.23.0"
  }
}
